<configuration>
  <connectionStrings>
    <add name="Usuarios" connectionString="Server=192.168.1.10;Database=usuarios;Uid=root;Pwd=aritel;" providerName="MySql.Data.MySqlClient" />
    <add name="Ariges" connectionString="Server=192.168.1.10;Database=ariges1;Uid=root;Pwd=aritel;" providerName="MySql.Data.MySqlClient" />
  </connectionStrings>
  <system.web>
    <compilation debug="true" targetFramework="4.0" />
    <httpRuntime />
    <pages controlRenderingCompatibilityVersion="4.0">
      <controls>
        <add tagPrefix="telerik" namespace="Telerik.Web.UI" assembly="Telerik.Web.UI" />
      </controls>
    </pages>
    <httpHandlers>
      <add path="ChartImage.axd" type="Telerik.Web.UI.ChartHttpHandler" verb="*" validate="false" />
